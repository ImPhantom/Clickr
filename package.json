{
	"name": "clickr",
	"productName": "Clickr",
	"version": "1.0.0",
	"description": "The slightly heavyweight, but undeniably beautiful auto-clicker. Built with Electron & Nut.js",
	"main": ".webpack/main",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"rebuild": "electron-rebuild",
		"lint": "echo \"No linting configured\""
	},
	"keywords": [],
	"author": {
		"name": "Phantom",
		"email": "dan@xeno.team",
		"url": "https://xeno.gg/"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"packagerConfig": {
				"icon": ".webpack/main/icons/icon"
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "clickr"
					}
				},
				{
					"name": "@electron-forge/maker-zip",
					"platforms": [
						"darwin"
					]
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": {}
				},
				{
					"name": "@electron-forge/maker-rpm",
					"config": {}
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-webpack",
					{
						"devContentSecurityPolicy": "default-src 'self' https://fonts.googleapis.com https://fonts.gstatic.com 'unsafe-eval' 'unsafe-inline';img-src * 'self' data:;media-src 'self' data:;",
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./src/renderer/index.html",
									"js": "./src/renderer/renderer.js",
									"name": "main_window",
									"preload": {
										"js": "./src/main/preload.js"
									}
								}
							]
						}
					}
				]
			]
		}
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.59",
		"@electron-forge/maker-deb": "^6.0.0-beta.59",
		"@electron-forge/maker-rpm": "^6.0.0-beta.59",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.59",
		"@electron-forge/maker-zip": "^6.0.0-beta.59",
		"@electron-forge/plugin-webpack": "6.0.0-beta.59",
		"@tailwindcss/forms": "^0.4.0",
		"@vercel/webpack-asset-relocator-loader": "1.6.0",
		"autoprefixer": "^10.4.0",
		"copy-webpack-plugin": "^10.2.0",
		"css-loader": "^6.0.0",
		"electron": "16.0.5",
		"eslint": "^8.5.0",
		"file-loader": "^6.2.0",
		"node-loader": "^2.0.0",
		"postcss": "^8.4.5",
		"postcss-loader": "^6.2.1",
		"sass": "^1.45.1",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.0.0",
		"tailwindcss": "^3.0.8"
	},
	"dependencies": {
		"@nut-tree/nut-js": "^2.0.1-next.20220104155257",
		"ajv": "^8.8.2",
		"electron-squirrel-startup": "^1.0.0",
		"electron-store": "^8.0.1"
	}
}
